{
  "name": "kiosk-frontend",
  "version": "1.0.0",
  "description": "Web application to interact with the rest of the kiosk",
  "repository": {
    "type": "git",
    "url": "https://github.com/vanvalenlab/kiosk-frontend.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf ./dist",
    "build:client": "webpack --mode production --config webpack.prod.js",
    "build:server": "babel -d ./dist/server ./server -s",
    "build": "yarn clean && yarn build:server && yarn build:client",
    "dev:server": "nodemon --exec babel-node ./server/index.js",
    "dev:client": "webpack-dev-server --mode development --open --config webpack.dev.js",
    "start": "concurrently --kill-others-on-fail \"yarn dev:server\" \"yarn dev:client\"",
    "start:prod": "yarn build && DEBUG='express:*' NODE_ENV='production' node ./dist/server/index.js"
  },
  "author": "The Van Valen Lab",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@google-cloud/storage": "2.0.3",
    "@loadable/component": "^5.10.1",
    "@material-ui/core": "3.1.0",
    "@material-ui/icons": "3.0.1",
    "@material-ui/lab": "3.0.0-alpha.16",
    "aws-sdk": "2.310.0",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-winston": "3.0.0",
    "fg-loadcss": "2.0.1",
    "helmet": "3.13.0",
    "http-status": "1.2.0",
    "joi": "13.6.0",
    "morgan": "1.9.1",
    "multer": "1.3.1",
    "multer-s3": "2.7.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-dropzone": "4.3.0",
    "react-hot-loader": "4.3.3",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-tabs": "2.2.2",
    "redis": "2.8.0",
    "uuid": "^3.3.2",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.1",
    "@babel/node": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "axios": "^0.18.1",
    "babel-loader": "8.0.2",
    "concurrently": "4.0.1",
    "css-loader": "^2.1.1",
    "eslint": "5.16.0",
    "eslint-plugin-react": "7.13.0",
    "html-webpack-plugin": "3.2.0",
    "nodemon": "1.18.4",
    "style-loader": "0.21.0",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.14"
  }
}
